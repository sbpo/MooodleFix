{
  "name": "AAU Moodle FIX",
  "description" : "This extension will shine up moodle for students at Aalborg University!",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "hello.html",
    "default_title": "I fix moodle up"
  },
  "icons": { "32": "icon32.png",
    "64": "icon64.png",
   "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://www.moodle.aau.dk/*", "http://www.moodle.aau.dk/*"],
      "css": ["moodlestyle.css"],
      "js": ["myscript.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ]
}
